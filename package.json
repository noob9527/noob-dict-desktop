  "private": true,
  "main": "build/electron.js",
  "homepage": "./",
  "author": {
    "name": "Your Name",
    "email": "your.email@domain.com",
    "url": "https://your-website.com"
  },
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "electron-is-dev": "^1.1.0",
    "electron-timber": "^0.5.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-scripts": "^3.1.2",
    "typescript": "3.6.3"
  },
  "scripts": {
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "electron:start": "parcel watch ./electron/main.ts --no-source-maps --target electron --out-dir build --out-file electron.js",
    "electron:build": "parcel build ./electron/main.ts --no-source-maps --target electron --out-dir build --out-file electron.js --no-minify",
    "start": "concurrently \"npm run electron:start\" \"BROWSER=none npm run react:start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "npm run react:build && npm run electron:build  && electron-builder build --linux"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.5.0",
    "parcel-bundler": "^1.12.3",
    "wait-on": "^3.3.0"
  }
}
